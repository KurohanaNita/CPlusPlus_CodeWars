#include <string>

std::string countSheep(int number) {
    std::string sheepSum = " " ;
    for (int i = 0; i <= number; ++i){
        std::string count = reinterpret_cast<const char *>(i);
        sheepSum += (count + " sheep...");
    }
    return sheepSum;
}

int main () {
    countSheep(0);
    countSheep(1);
    countSheep(2);
    countSheep(3);
    countSheep(4);
    countSheep(5);

    return 0;
}
